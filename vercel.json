{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "FLASK_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "app.py": {
      "maxDuration": 300
    }
  }
}